{
  "name": "ju1ius/fusbup",
  "description": "Supafast Public Suffix matching",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "public suffix list",
    "top level domain",
    "tld",
    "psl",
    "icann",
    "domain",
    "cookie"
  ],
  "authors": [
    {
      "name": "ju1ius",
      "email": "jules.bernable@gmail.com"
    }
  ],
  "scripts": {
    "test": "tools/phpunit.phar",
    "coverage": "rm -rf tmp/coverage && XDEBUG_MODE=coverage tools/phpunit.phar --coverage-html=tmp/coverage",
    "update-list": "php scripts/update-list.php",
    "update-psl-tests": "php scripts/update-psl-tests.php"
  },
  "autoload": {
    "psr-4": {
      "ju1ius\\FusBup\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ju1ius\\FusBup\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "symfony/stopwatch": "^6.2"
  }
}
